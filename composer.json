    "keywords": [
      "php code generator",
      "code generator",
      "generator",
      "generation"
    ],
    "homepage": "https://github.com/Speicher210/CodeGenerator",
    "license": "MIT",
    "authors": [
        {
          "name":"Dragos Protung",
          "email": "dragos@protung.ro",
          "role":"Developer"
        }
    ],
    "require": {
        "php" : ">=5.4",
        "wingu/reflection":"~1.0"
    },
    "require-dev": {
